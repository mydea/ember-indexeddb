{
  "name": "ember-indexeddb",
  "version": "3.0.0-beta.1",
  "description": "A utility for using IndexedDB with ember & ember-data",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mydea/ember-indexeddb.git"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "run-prettier": "prettier --write \"**/*.{js,scss}\"",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:ember-one": "ember try:one",
    "release": "release-it"
  },
  "dependencies": {
    "@ember/test-waiters": "^2.4.4",
    "@embroider/macros": "^0.45.0",
    "dexie": "^3.0.3",
    "ember-auto-import": "^2.0.0",
    "ember-cli-babel": "^7.26.5",
    "ember-concurrency": "^1.3.0 || ^2.0.3"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.5",
    "@embroider/test-setup": "~0.40.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@release-it/conventional-changelog": "~2.0.1",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.26.1",
    "ember-cli-app-version": "~5.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-release-tag": "^0.1.5",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-yuidoc": "^0.9.1",
    "ember-data": "~3.27.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.1",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-router-service-refresh-polyfill": "~0.1.0",
    "ember-source": "~3.27.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-try": "^1.4.0",
    "eslint": "~7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.4.2",
    "eslint-plugin-ember-es6-class": "^0.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "loader.js": "^4.7.0",
    "prettier": "^2.3.0",
    "qunit": "^2.15.0",
    "qunit-dom": "^1.6.0",
    "release-it": "^14.6.2",
    "webpack": "^5.37.0",
    "yuidoc-ember-theme": "^2.0.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "volta": {
    "node": "14.17.0",
    "yarn": "1.22.10"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://mydea.github.io/ember-indexeddb/docs/"
  },
  "homepage": "https://github.com/mydea/ember-indexeddb"
}
