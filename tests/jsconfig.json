{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "ember-indexeddb/*": ["addon/*"],
      "dummy/*": ["tests/dummy/*"],
      "dummy/tests*": ["tests/*"],
      "*": ["types/*"]
    },
    "experimentalDecorators": true,
    "target": "esnext",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node"
  },
  "exclude": ["node_modules", "dist", "tmp", "coverage", "vendor", ".git"]
}
